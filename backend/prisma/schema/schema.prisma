generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["omitApi", "relationJoins", "prismaSchemaFolder", "fullTextSearch"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

